{
  "shape": {
    "transform": [
      {
        "svgo": {
          "plugins": [
            {
              "removeDimensions": true
            },
            {
              "removeAttrs": { "attrs": "(fill|stroke)" }
            }
          ]
        }
      }
    ]
  },
  "mode": {
    "symbol": {
      "dest": "dist",
      "sprite": "esds-icons.svg",
      "example": false
    }
  }
}
