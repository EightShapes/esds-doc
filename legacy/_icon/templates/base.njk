<!doctype html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="x-ua-compatible" content="ie=edge">
        <title>{{ title }}</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        {% block head_content_prepend %}{% endblock head_content_prepend %}
        {% block head_content_append %}{% endblock head_content_append %}
    </head>
    <body>
        {% block body %}{% endblock body %}
        <script>
          window.ShadyDOM = { force: true };
        </script>
        <script src="/scripts/shadydom.min.js"></script>
        <script type="module" src="/scripts/esds-icon.js"></script>
        {% block closing_body_assets %}{% endblock closing_body_assets %}
    </body>
</html>
